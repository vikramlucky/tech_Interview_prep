import java.util.*;

public class Main {

    public static void main(String[] args) {
        // print 5 star
        System.out.println("*****");
        // print 3 space and 1 star
        System.out.println("   *");
        // print 2 space and 1 star
        System.out.println("  *");
        // print 1 space and 1 star
        System.out.println(" *");
        // print 5 star
        System.out.println("*****");
    }
}
