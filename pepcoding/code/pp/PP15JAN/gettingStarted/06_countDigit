import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);

        int num = scn.nextInt();

        // splitting num = num / 10;

        int count = 0;
        while (num != 0) {
            num = num / 10;
            count++;
        }

        // int count = 0;
        // for(; num != 0;) {
        //     num = num / 10;
        //     count = count + 1;
        // }

        System.out.println(count);
    }
}
