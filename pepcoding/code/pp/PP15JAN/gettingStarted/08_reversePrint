import java.util.*;

public class Main {

    public static void main(String[] args) {
        // write your code here  
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();

        for (; n != 0;) {
            int ld = n % 10; // last digit
            n = n / 10;
            System.out.println(ld);
        }
    }
}
