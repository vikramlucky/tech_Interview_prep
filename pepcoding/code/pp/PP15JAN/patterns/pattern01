import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);

        int n = scn.nextInt();

        int star = 1;

        for (int r = 1; r <= n; r++) {
            // print star, which are separated by tab
            for (int st = 1; st <= star; st++) {
                System.out.print("*\t");
            }

            // hit enter
            System.out.println();
            // increment in star
            star++;
        }
    }
}
