import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);

        // write ur code here
        int n = scn.nextInt();

        int star = n;
        for (int r = 1; r <= n; r++) {
            // print star
            for (int st = 1; st <= star; st++) {
                System.out.print("*\t");
            }
            // hit enter
            System.out.println();
            // decrease star count
            star--;
        }

    }
}
